#config PLATFORM CPU CPU_MPMD
#config PROCESSES 4
#config CORES 8
#config MODE debug

matrix<double,32,32,dist> as;

double init(int x, int y, double a){
	return (double) x + y + 1;
}

double square(double a){
	return a * a;
}

double sum(double a, double b){
	return a + b;
}

main{
	as.mapIndexInPlace(init());
	as.show();
	
	mkt::roi_start();
	
	as.mapInPlace(square());
	double fn = as.fold(0.0, sum());
	fn = std::sqrt(fn);
	
	mkt::roi_end();
	
	mkt::print("Frobenius norm is %.5f.\n", fn);
}
