#config PLATFORM CPU
#config PROCESSES 4
#config CORES 8
#config MODE debug

// data structures
matrix<int, 4, 4, dist> m = {1};

// user functions
int func(int r, int c, int i){
	return i + r;
}

// logic
main{
	m.show();
	mkt::roi_start();
	m.mapIndexInPlace(func());
	mkt::roi_end();
	m.show();
}
